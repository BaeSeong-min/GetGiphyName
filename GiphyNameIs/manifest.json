{
    "name": "Hello extension",
    "description": "나의 첫 확장 프로그램",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
        "default_popup": "main.html"
    },
    "content_scripts": [
        {
            "matches": ["https://www.instagram.com/*"],
            "js": ["main.js"]
        }
    ],
    "permissions": [
        "activeTab"
    ]
}